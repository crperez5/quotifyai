{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "Launch .NET API backend",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/app/backend/MinimalAPI.csproj",
        }        
    ]
}