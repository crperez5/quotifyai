{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build API",
			"command": "dotnet",
			"args": [
				"build",
				"backend.sln",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"type": "process",
			"group": "build",
			"problemMatcher": "$msCompile",
			"options": {
				"cwd": "${workspaceFolder}/app"
			}
		},		
	]
}